{
  "name": "@filezen/astro",
  "version": "0.1.0",
  "private": true,
  "description": "FileZen SDK for Astro applications",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "keywords": [
    "filezen",
    "astro",
    "file-upload",
    "typescript"
  ],
  "devDependencies": {
    "@repo/tsup-config": "*",
    "astro": "^5.10.1",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@filezen/js": ">=0.1.0",
    "astro": "^5.10.1"
  }
} 
